{
    "files.watcherExclude": {
        "**/.stack-work/**": true,
        "**/.stack-work-profile/**": true,
        "**/dist-newstyle/**": true
    },

    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.stack-work*": true,
        "**/dist-newstyle": true,
        "*.bak": true,
        "*~": true
    },
    "cSpell.words": [
        "SCARV",
        "YARVI"
    ],
    "cSpell.ignoreWords": [
        "cop",
        "palu",
        "scarv",
        "scarv cop palu",
        "timeit",
        "xcrypto"
    ]
}